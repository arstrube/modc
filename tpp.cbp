<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="tpp" />
		<Option pch_mode="2" />
		<Option compiler="cygwin" />
		<Build>
			<Target title="Debug">
				<Option output="./$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output="out/Debug/" />
				<Option type="1" />
				<Option compiler="cygwin" />
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++0x" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-Werror" />
			<Add directory="e:/cpputest/include" />
		</Compiler>
		<Linker>
			<Add library="CppUTest" />
			<Add library="CppUTestExt" />
			<Add directory="e:/cpputest/lib" />
		</Linker>
		<ExtraCommands>
			<Add after="./$(PROJECT_NAME).exe" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="CppUTestMain.cpp" />
		<Unit filename="tpp/Soundex.h" />
		<Unit filename="tpp/SoundexTest.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
